<?xml version="1.0" encoding="UTF-8"?>
<KeyConfig>
    <!-- DUKPT (Derived Unique Key Per Transaction) Configuration -->
    <DUKPT>
        <KeyType>BDK</KeyType>
        <BDK>0123456789ABCDEFFEDCBA9876543210</BDK>
        <KSI>FFFF9876543210E00000</KSI>
    </DUKPT>

    <!-- PIN Entry Configuration -->
    <PIN>
        <PINFormat>ISO9564-0</PINFormat>
        <MaxPINLength>12</MaxPINLength>
        <MinPINLength>4</MinPINLength>
        <PINBypassAllowed>false</PINBypassAllowed>
    </PIN>
</KeyConfig>
